{
  "name": "@wayke-se/ecom",
  "version": "2.2.0-bankid-0",
  "description": "A SDK to utilize Wayke e-commerce APIs",
  "main": "src/index.ts",
  "typings": "src/index.d.ts",
  "scripts": {
    "build": "pika build",
    "prebuild": "npm run clean",
    "build:tsc": "tsc",
    "prebuild:tsc": "npm run clean",
    "clean": "rimraf ./dist ./pkg ./coverage",
    "lint": "eslint -c .eslintrc.js --ext .ts 'src/**/*.ts'",
    "prettify": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "npm run prettify && npm run type-check && npm run lint && npm run test",
    "test": "jest --coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=!src/index.ts --verbose",
    "pretest": "rimraf ./coverage",
    "type-check": "tsc --noEmit --skipLibCheck",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wayke/wayke-ecom-js.git"
  },
  "keywords": [
    "wayke",
    "ecom"
  ],
  "author": "Björn Vikström",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wayke/wayke-ecom-js/issues"
  },
  "homepage": "https://github.com/wayke/wayke-ecom-js#readme",
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "autofixture": "^1.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "jest-theories": "1.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ]
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "test/*",
            "@types/*",
            "**/*.spec.ts"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "files": [
    "dist-*/"
  ],
  "dependencies": {}
}
